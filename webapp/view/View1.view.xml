<mvc:View
    controllerName="com.sap.imayl.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:tnt="sap.tnt"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.f"
    xmlns:layout="sap.ui.layout.form"
    xmlns:m="sap.m"
>
    <HBox
        id="_IDGenVBox"
        fitContainer="true"
        height="100%"
        width="100%"
    >
        <VBox
            id="_IDGenHBox"
            fitContainer="true"
            height="100%"
            width="auto"
            renderType="Bare"
        >
            <VBox id="_IDGenVBox7" width="100%" height="10%" class="logoTransition">
                <Image
                id="_IDGenImage"
                class="logoTransition"
                src="/img/mlogo.png" 
                decorative="false"
                alt="Company Logo"
            />
            </VBox>

            <tnt:SideNavigation
                id="sideNavigation"
                expanded="false"
            >
                <tnt:NavigationList
                    id="_IDGenNavigationList1"
                    itemSelect=".onItemSelect"
                >
                    <tnt:NavigationListItem
                        id="_IDGenNavigationListItem"
                        key="all"
                        text="All Packages"
                        icon="sap-icon://soccer"
                    />
                    <tnt:NavigationListItem
                        id="_IDGenNavigationListItem1"
                        key="receive"
                        text="Recieve a package"
                        icon="sap-icon://add-product"
                    />
                    <tnt:NavigationListItem
                        id="_IDGenNavigationListItem2"
                        key="mine"
                        text="My Packages"
                        icon="sap-icon://customer-history"
                    />
                    <tnt:NavigationListItem
                        id="_IDGenNavigationListItem3"
                        text="Reports"
                        icon="sap-icon://inspection"
                        expanded="false"
                    >
                        <tnt:NavigationListItem
                            id="_IDGenNavigationListItem4"
                            key="sla"
                            text="SLA Compliance"
                            icon="sap-icon://org-chart"
                        />
                        <tnt:NavigationListItem
                            id="_IDGenNavigationListItem5"
                            key="pending"
                            text="Pending Deliveries"
                            icon="sap-icon://org-chart"
                        />
                        <tnt:NavigationListItem
                            id="_IDGenNavigationListItem6"
                            key="researched"
                            text="Researched Packages"
                            icon="sap-icon://org-chart"
                        />
                        <tnt:NavigationListItem
                            id="_IDGenNavigationListItem7"
                            key="productivity"
                            text="User Productivity"
                            icon="sap-icon://org-chart"
                        />
                    </tnt:NavigationListItem>
                </tnt:NavigationList>

                <tnt:fixedItem>
                    <tnt:NavigationList id="_IDGenNavigationList2"
                    itemSelect=".onItemSelect">
                        <tnt:NavigationListItem
                            id="_IDGenNavigationListItem8"
                            text="Set Up"
                            icon="sap-icon://wrench">
                            <tnt:NavigationListItem
                            text="User Management"
                            key="user"
                            icon="sap-icon://org-chart"
                        />
                        </tnt:NavigationListItem>
                        <tnt:NavigationListItem
                            id="_IDGenNavigationListItem9"
                            selectable="false"
                            href="https://sap.com"
                            target="_blank"
                            text="User Log"
                            icon="sap-icon://org-chart"
                        />
                        <tnt:NavigationListItem
                            id="_IDGenNavigationListItem10"
                            text="Email Editor"
                            key="email"
                            icon="sap-icon://email-read"
                        />
                    </tnt:NavigationList>
                </tnt:fixedItem>
            </tnt:SideNavigation>
        </VBox>

        <VBox
            id="_IDGenVBox5"
            fitContainer="true"
            height="100%"
            width="100%"
            renderType="Bare"
        >
            <tnt:ToolHeader id="_IDGenToolHeader" height="12%" width="100%">
                <Button
                    id="_IDGenButton"
                    icon="sap-icon://menu2"
                    press=".onCollapseExpandPress"
                />
                <ToolbarSpacer id="_IDGenToolbarSpacer" />
                <Button
                    id="_IDGenButton1"
                    icon="sap-icon://action-settings"
                />
                <Text
                    id="_IDGenText"
                    text="Sasank Akula"
                />
            </tnt:ToolHeader>

            <f:DynamicPage id="_IDGenDynamicPage1">
                <f:title>
                    <f:DynamicPageTitle id="_IDGenDynamicPageTitle">
                        <f:heading>
                            <Text
                                id="_IDGenText1"
                                text="{/propertyName}"
                            />
                        </f:heading>
                    </f:DynamicPageTitle>
                </f:title>
                <f:content>
                    <VBox
                        id="pageContainer"
                        fitContainer="true"
                    />
                </f:content>
            </f:DynamicPage>

            <Bar id="_IDGenBar">
                <contentLeft>
                    <Text
                        id="_IDGenText3"
                        text="Â© IMAYL, Inc. All rights reserved."
                    />
                </contentLeft>
            </Bar>
        </VBox>
    </HBox>
</mvc:View>
